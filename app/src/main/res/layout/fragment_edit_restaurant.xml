<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.owner.lacochina.EditRestaurantFragment">

    <!-- TODO: Update blank fragment layout -->

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text="Telephone"
        android:ems="10"
        android:id="@+id/resTelephoneEdit"
        android:layout_marginTop="32dp"
        android:layout_below="@+id/resAddrEdit"
        android:layout_alignStart="@+id/resTypeEdit" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text="Reputation"
        android:ems="10"
        android:id="@+id/resRepEdit"
        android:layout_below="@+id/resTelephoneEdit"
        android:layout_alignStart="@+id/resTelephoneEdit" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text="Delivery"
        android:ems="10"
        android:id="@+id/resDeliveryEdit"
        android:layout_below="@+id/resRepEdit"
        android:layout_alignStart="@+id/resRepEdit" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text="Type"
        android:ems="10"
        android:id="@+id/resTypeEdit"
        android:layout_below="@+id/resAddrEdit"
        android:layout_alignStart="@+id/resAddrEdit" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text="Name"
        android:ems="10"
        android:id="@+id/resNameEdit"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text="Address"
        android:ems="10"
        android:id="@+id/resAddrEdit"
        android:layout_below="@+id/resNameEdit"
        android:layout_alignParentEnd="true" />

    <TextView
        android:text="Nombre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="55dp"
        android:id="@+id/textView15"
        android:layout_alignBaseline="@+id/resNameEdit"
        android:layout_alignBottom="@+id/resNameEdit"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="Direccion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/resNameEdit"
        android:layout_alignStart="@+id/textView15"
        android:layout_marginTop="10dp"
        android:id="@+id/textView16" />

    <TextView
        android:text="Categoria"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/resAddrEdit"
        android:layout_alignStart="@+id/textView16"
        android:layout_marginTop="8dp"
        android:id="@+id/textView17" />

    <TextView
        android:text="Telefono"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/resTypeEdit"
        android:layout_alignStart="@+id/textView17"
        android:id="@+id/textView18" />

    <TextView
        android:text="Reputacion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/resTelephoneEdit"
        android:layout_marginTop="9dp"
        android:id="@+id/textView19"
        android:layout_alignStart="@+id/textView18" />

    <TextView
        android:text="Entrega dom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/resRepEdit"
        android:layout_alignStart="@+id/textView19"
        android:layout_marginTop="10dp"
        android:id="@+id/textView20" />

    <Button
        android:text="Regresar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button5"
        android:layout_marginStart="48dp"
        android:layout_alignBaseline="@+id/button4"
        android:layout_alignBottom="@+id/button4"
        android:layout_alignStart="@+id/resDeliveryEdit" />

    <Button
        android:text="Editar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button4"
        android:layout_marginTop="41dp"
        android:layout_marginStart="20dp"
        android:layout_below="@+id/resDeliveryEdit"
        android:layout_alignStart="@+id/textView20" />
</RelativeLayout>
